{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/frontend/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "my-app",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/frontend/public/icon.png"
    }
  },
  "cli": {
    "frontendLibrary": {
      "patchFile": "/frontend/public/index.html",
      "devUrl": "http://localhost:8080"
    },
    "binaryName": "my-app",
    "resourcesPath": "/frontend/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/frontend/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0"
  }
}